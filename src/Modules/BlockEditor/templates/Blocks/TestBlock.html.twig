<blockquote class="blockquote{% if editorBlock.alignment == 'center' %} text-center{% endif %}">
  <p>{{ editorBlock.text }}</p>
  <footer class="blockquote-footer">{{ editorBlock.author }}</footer>
  {% if editorBlock.imagePosition == 'left' %}
      image position = LEFT
    {% else %}
      image position = RIGHT
  {% endif %}
  image position: {{ editorBlock.imagePosition }}
  {{ dump(editorBlock) }}
</blockquote>
